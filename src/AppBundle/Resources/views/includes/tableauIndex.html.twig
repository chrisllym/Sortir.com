<table class="table table-dark col-12">
    <thead class="table-dark">
    <tr>
        <th>Nom de la sortie</th>
        <th>Date de la sortie</th>
        <th>Clôture</th>
        <th>Inscrits/Places</th>
        <th>État</th>
        <th>Inscrit</th>
        <th>Organisateur</th>
        <th>Actions</th>

    </tr>
    </thead>
        {% for sorties in sortie %}
        <tbody>
        <tr>
            <td scope="col">{{ sorties.nom }}</td>
            <td scope="col">{{ sorties.datedebut }}</td>
            <td scope="col">{{ sorties.datecloture }}</td>
            <td scope="col">{{ sorties.nbinscriptionsmax }}</td>
            <td scope="col">{{ sorties.etatsortie }}</td>
            <td scope="col">{{ sorties.nom }}</td>
            <td scope="col">{{ sorties.organisateur }}</td>
            <td scope="col">{</td>
        </tr>
        {% endfor %}
        </tbody>
</table>